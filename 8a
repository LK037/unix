#include<stdio.h>
#include<fcntl.h>
#include<unistd.h>
#include<dirent.h>
#include<sys/stat.h>
int main
{
int fd1=0,fd2=0;
char buf[50];

if((fd1=open("t12.txt",0_RDWR,0))<0)
printf("File open error");
fd2=dup(fd1);

read(fd1, 0L, SEEK_END);
write(fd2, buf, 12);

return 0;
}

vi t12.txt
cc 8a.cc
./a.out
cat t12.txt
